{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "payroll-nyc-ali"
		},
		"ls_adl_nycpayroll_data_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adl_nycpayroll_data'"
		},
		"ls_sql_nycpayroll_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_nycpayroll'"
		},
		"ls_adl_nycpayroll_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsnycpayrollalim.dfs.core.windows.net/"
		},
		"ls_sql_nycpayroll_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "tcp:payroll-nyc.database.windows.net,1433"
		},
		"ls_sql_nycpayroll_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db_nycpayroll"
		},
		"ls_sql_nycpayroll_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "payrollnyc"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "payroll-nyc-ali_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adl_nycpayroll_data_accountKey": {
						"value": "[parameters('ls_adl_nycpayroll_data_accountKey')]"
					},
					"ls_sql_nycpayroll_password": {
						"value": "[parameters('ls_sql_nycpayroll_password')]"
					},
					"ls_adl_nycpayroll_data_properties_typeProperties_url": {
						"value": "[parameters('ls_adl_nycpayroll_data_properties_typeProperties_url')]"
					},
					"ls_sql_nycpayroll_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_nycpayroll_properties_typeProperties_server')]"
					},
					"ls_sql_nycpayroll_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_nycpayroll_properties_typeProperties_database')]"
					},
					"ls_sql_nycpayroll_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_nycpayroll_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}